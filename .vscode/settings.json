{
  "files.exclude": {
    "amplify/.config": true,
    "amplify/**/*-parameters.json": true,
    "amplify/**/amplify.state": true,
    "amplify/**/transform.conf.json": true,
    "amplify/#current-cloud-backend": true,
    "amplify/backend/amplify-meta.json": true,
    "amplify/backend/awscloudformation": true
  },
  "journal.templates": [
    {
      "name": "memo",
      "template": "- MEMO ${localTime}: ${input}"
    },
    {
      "name": "task",
      "template": "- [] ${d:LL} - Task: ${input}"
    },
    {
      "name": "entry",
      "template": "# ${d:dddd, MMMM DD YYYY}\n\n## Done\n\n## Todo\n\n## Notes\n\n"
    },
    {
      "name": "time",
      "template": "${localTime}"
    },
    {
      "name": "note",
      "template": "# ${input}\n\n${tags}\n"
    },
    {
      "name": "files",
      "template": "- NOTE: [${title}](${link})"
    }
],
"vscode-journal-view.showNonJournalFiles": true,
"journal.base": "/Users/roxberry/Journal/journal-main"}